{
  "name": "eofol5",
  "version": "0.0.1",
  "scripts": {
    "start": "node ./script/start.js",
    "build": "node ./script/build.js",
    "serve": "node ./script/serve.js",
    "clean": "node ./script/clean.js",
    "reinstall": "node ./script/reinstall.js",
    "reinstall-f": "node ./script/reinstall.js -f",
    "deploy": "node ./script/deploy.js",
    "release": "npm run clean && npm run build && npm run deploy",
    "check": "npx eslint -c eslint.config.mjs .",
    "check-fix": "npx eslint -c eslint.config.mjs --fix .",
    "prettify": "npx prettier --config .prettierrc --write .",
    "lint": "npm run check-fix && npm run prettify",
    "depcheck": "npx depcheck",
    "docker-build": "docker build -t eofol5-test ./docker",
    "docker-run": "docker run -p 3000:3000 eofol5-test",
    "docker-stop": "bash ./docker/docker-stop.sh",
    "docker-test": "npm run docker-stop && npm run docker-build && npm run docker-run"
  },
  "author": "Jakub Eliáš <wurducius@gmail.com>",
  "license": "MIT",
  "description": "All inclusive web framework with zero configuration, batteries included!",
  "dependencies": {
    "@eofol/eofol-dev-utils": "^0.2.26",
    "dotenv": "^16.4.7",
    "eofol-dev-server": "^0.0.1",
    "eofol-head": "1.0.0",
    "html-minifier-terser": "^7.2.0",
    "node-html-parser": "^7.0.1",
    "open": "^10.1.0",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "uglify-js": "^3.19.3",
    "watchpack": "^2.4.2",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "typescript-eslint": "^8.19.1"
  }
}
